    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "google-auth-library": "^8.0.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
